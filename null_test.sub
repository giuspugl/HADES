# batch_maps.sub -- run map creation for each patch

universe		= vanilla
executable              = /data/ohep2/hades/null_test.sh
getenv			= true
arguments               = "$(Process)"
should_transfer_files   = IF_NEEDED
when_to_transfer_output = ON_EXIT
request_memory		= 4000
log                     = /dev/null
output			= null_test.out
error			= null_test.error
notification		= never
queue 5000
