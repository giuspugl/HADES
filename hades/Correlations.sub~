# Correlations.sub -- run map creation for each patch and frequency

universe		= vanilla
executable              = /data/ohep2/hades/Correlations.sh
getenv			= true
arguments               = "$(Process)"
should_transfer_files   = IF_NEEDED
when_to_transfer_output = ON_EXIT
request_memory		= 2500
log                     = /dev/null
output			= correlations.out
error			= correlations.error
notification		= never
queue 2500
